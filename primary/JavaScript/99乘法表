		<script type="text/javascript">
			for(var i=1; i<=9; i++){
				document.write("<br>");
				for(var j=1;j<=i;j++){
					sum=i*j;
					document.write(i,"*",j,"=",sum,"	");
				}
			}
		</script>
	</body>
